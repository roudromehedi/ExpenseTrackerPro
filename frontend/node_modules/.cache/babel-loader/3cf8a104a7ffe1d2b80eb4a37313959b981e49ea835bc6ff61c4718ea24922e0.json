{"ast":null,"code":"import Vue from \"vue\";\nimport axios from \"axios\";\nVue.use(axios);\nexport default {\n  name: \"EmployeeCrud\",\n  data() {\n    return {\n      result: {},\n      employee: {\n        _id: \"\",\n        name: \"\",\n        address: \"\",\n        phone: \"\"\n      }\n    };\n  },\n  created() {\n    this.EmployeeLoad();\n  },\n  mounted() {\n    console.log(\"mounted() called.......\");\n  },\n  methods: {\n    EmployeeLoad() {\n      var page = \"http://localhost:8000/user/getAll\";\n      axios.get(page).then(({\n        data\n      }) => {\n        console.log(data);\n        this.result = data.data;\n      });\n    },\n    save() {\n      if (this.employee._id == \"\") {\n        this.saveData();\n      } else {\n        this.updateData();\n      }\n    },\n    saveData() {\n      axios.post(\"http://localhost:8000/user/create\", this.employee).then(() => {\n        alert(\"saveddddd\");\n        this.EmployeeLoad();\n      });\n    },\n    edit(employee) {\n      this.employee = employee;\n    },\n    updateData() {\n      var editrecords = \"http://localhost:8000/user/update/\" + this.employee._id;\n      axios.patch(editrecords, this.employee).then(() => {\n        this.employee.name = \"\";\n        this.employee.address = \"\", this.employee.phone = \"\";\n        this.id = \"\";\n        alert(\"Updated!!!\");\n        this.EmployeeLoad();\n      });\n    },\n    remove(employee) {\n      var url = `http://localhost:8000/user/delete/${employee._id}`;\n      axios.delete(url);\n      alert(\"Deleteddd\");\n      this.EmployeeLoad();\n    }\n  }\n};","map":{"version":3,"names":["Vue","axios","use","name","data","result","employee","_id","address","phone","created","EmployeeLoad","mounted","console","log","methods","page","get","then","save","saveData","updateData","post","alert","edit","editrecords","patch","id","remove","url","delete"],"sources":["src/components/HelloWorld.vue"],"sourcesContent":["<template>\n  <div>\n    <h2>Employee Registation</h2>\n    <form @submit.prevent=\"save\">\n      <div class=\"form-group\">\n        <label>Employee name</label>\n        <input\n          type=\"text\"\n          v-model=\"employee.name\"\n          class=\"form-control\"\n          placeholder=\"Employee name\"\n        />\n      </div>\n      <div class=\"form-group\">\n        <label>Employee Address</label>\n        <input\n          type=\"text\"\n          v-model=\"employee.address\"\n          class=\"form-control\"\n          placeholder=\"Employee Address\"\n        />\n      </div>\n\n      <div class=\"form-group\">\n        <label>Mobile</label>\n        <input\n          type=\"text\"\n          v-model=\"employee.phone\"\n          class=\"form-control\"\n          placeholder=\"Mobile\"\n        />\n      </div>\n\n      <button type=\"submit\" class=\"btn btn-primary\">Save</button>\n    </form>\n\n    <h2>Employee View</h2>\n    <table class=\"table table-dark\">\n      <thead>\n        <tr>\n          <th scope=\"col\">ID</th>\n          <th scope=\"col\">Employee Name</th>\n          <th scope=\"col\">Address</th>\n          <th scope=\"col\">Mobile</th>\n          <th scope=\"col\">Option</th>\n        </tr>\n      </thead>\n      <tbody>\n        <tr v-for=\"employee in result\" v-bind:key=\"employee._id\">\n          <td>{{ employee._id }}</td>\n          <td>{{ employee.name }}</td>\n          <td>{{ employee.address }}</td>\n          <td>{{ employee.phone }}</td>\n          <td>\n            <button\n              type=\"button\"\n              class=\"btn btn-warning\"\n              @click=\"edit(employee)\"\n            >\n              Edit\n            </button>\n            <button\n              type=\"button\"\n              class=\"btn btn-danger\"\n              @click=\"remove(employee)\"\n            >\n              Delete\n            </button>\n          </td>\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n\n<script>\nimport Vue from \"vue\";\nimport axios from \"axios\";\n\nVue.use(axios);\n\nexport default {\n  name: \"EmployeeCrud\",\n  data() {\n    return {\n      result: {},\n      employee: {\n        _id: \"\",\n        name: \"\",\n        address: \"\",\n        phone: \"\",\n      },\n    };\n  },\n  created() {\n    this.EmployeeLoad();\n  },\n  mounted() {\n    console.log(\"mounted() called.......\");\n  },\n\n  methods: {\n    EmployeeLoad() {\n      var page = \"http://localhost:8000/user/getAll\";\n      axios.get(page).then(({ data }) => {\n        console.log(data);\n        this.result = data.data;\n      });\n    },\n\n    save() {\n      if (this.employee._id == \"\") {\n        this.saveData();\n      } else {\n        this.updateData();\n      }\n    },\n    saveData() {\n      axios\n        .post(\"http://localhost:8000/user/create\", this.employee)\n        .then(() => {\n          alert(\"saveddddd\");\n          this.EmployeeLoad();\n        });\n    },\n    edit(employee) {\n      this.employee = employee;\n    },\n    updateData() {\n      var editrecords =\n        \"http://localhost:8000/user/update/\" + this.employee._id;\n      axios.patch(editrecords, this.employee).then(() => {\n        this.employee.name = \"\";\n        (this.employee.address = \"\"), (this.employee.phone = \"\");\n        this.id = \"\";\n        alert(\"Updated!!!\");\n        this.EmployeeLoad();\n      });\n    },\n\n    remove(employee) {\n      var url = `http://localhost:8000/user/delete/${employee._id}`;\n      axios.delete(url);\n      alert(\"Deleteddd\");\n      this.EmployeeLoad();\n    },\n  },\n};\n</script>\n"],"mappings":"AA4EA,OAAAA,GAAA;AACA,OAAAC,KAAA;AAEAD,GAAA,CAAAE,GAAA,CAAAD,KAAA;AAEA;EACAE,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;MACAC,QAAA;QACAC,GAAA;QACAJ,IAAA;QACAK,OAAA;QACAC,KAAA;MACA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,YAAA;EACA;EACAC,QAAA;IACAC,OAAA,CAAAC,GAAA;EACA;EAEAC,OAAA;IACAJ,aAAA;MACA,IAAAK,IAAA;MACAf,KAAA,CAAAgB,GAAA,CAAAD,IAAA,EAAAE,IAAA;QAAAd;MAAA;QACAS,OAAA,CA