{"ast":null,"code":"import Vue from \"vue\";\nimport axios from \"axios\";\nVue.use(axios);\nexport default {\n  name: \"EmployeeCrud\",\n  data() {\n    return {\n      result: {},\n      employee: {\n        _id: \"\",\n        name: \"\",\n        address: \"\",\n        phone: \"\"\n      }\n    };\n  },\n  created() {\n    this.EmployeeLoad();\n  },\n  mounted() {\n    console.log(\"mounted() called.......\");\n  },\n  methods: {\n    EmployeeLoad() {\n      var page = \"http://localhost:8000/user/getAll\";\n      axios.get(page).then(({\n        data\n      }) => {\n        console.log(data);\n        this.result = data.data;\n      });\n    },\n    save() {\n      if (this.employee._id == \"\") {\n        this.saveData();\n      } else {\n        this.updateData();\n      }\n    },\n    saveData() {\n      axios.post(\"http://localhost:8000/user/create\", this.employee).then(() => {\n        alert(\"saveddddd\");\n        this.EmployeeLoad();\n      });\n    },\n    edit(employee) {\n      this.employee = employee;\n    },\n    updateData() {\n      var editrecords = \"http://localhost:8000/user/update/\" + this.employee._id;\n      axios.patch(editrecords, this.employee).then(() => {\n        this.employee.name = \"\";\n        this.employee.address = \"\", this.employee.phone = \"\";\n        this.id = \"\";\n        alert(\"Updated!!!\");\n        this.EmployeeLoad();\n      });\n    },\n    remove(employee) {\n      var url = `http://localhost:8000/user/delete/${employee._id}`;\n      axios.delete(url);\n      alert(\"Deleteddd\");\n      this.EmployeeLoad();\n    }\n  }\n};","map":{"version":3,"names":["Vue","axios","use","name","data","result","employee","_id","address","phone","created","EmployeeLoad","mounted","console","log","methods","page","get","then","save","saveData","updateData","post","alert","edit","editrecords","patch","id","remove","url","delete"],"sources":["src/components/HelloWorld.vue"],"sourcesContent":["<template>\n  <v-container>\n    <v-row>\n      <v-col cols=\"12\" md=\"6\">\n        <v-card>\n          <v-card-title>\n            <h2 class=\"text-center\">Employee Registration</h2>\n          </v-card-title>\n          <v-card-text>\n            <v-form @submit.prevent=\"save\">\n              <v-text-field\n                v-model=\"employee.name\"\n                label=\"Employee Name\"\n                required\n                :rules=\"[\n                  (v) => !!v || 'Name is required',\n                  (v) =>\n                    (v && v.length >= 50) ||\n                    'Name must be less than 50 characters',\n                ]\"\n              ></v-text-field>\n\n              <v-text-field\n                v-model=\"employee.address\"\n                label=\"Employee Address\"\n                required\n              ></v-text-field>\n\n              <v-text-field\n                v-model=\"employee.phone\"\n                label=\"Mobile\"\n                required\n              ></v-text-field>\n\n              <v-btn type=\"submit\" color=\"primary\">Save</v-btn>\n            </v-form>\n          </v-card-text>\n        </v-card>\n      </v-col>\n      <v-col cols=\"12\" md=\"6\">\n        <v-simple-table>\n          <template v-slot:default>\n            <thead>\n              <tr>\n                <th class=\"text-left\">Name</th>\n                <th class=\"text-left\">Address</th>\n                <th class=\"text-left\">Phone</th>\n                <th></th>\n              </tr>\n            </thead>\n            <tbody>\n              <tr v-for=\"(employee, index) in result\" :key=\"index\">\n                <td>{{ employee.name }}</td>\n                <td>{{ employee.address }}</td>\n                <td>{{ employee.phone }}</td>\n                <td class=\"text-right\">\n                  <v-btn color=\"warning\" class=\"mr-2\" @click=\"edit(employee)\">\n                    Edit\n                  </v-btn>\n                  <v-btn color=\"error\" @click=\"remove(employee)\">Delete</v-btn>\n                </td>\n              </tr>\n            </tbody>\n          </template>\n        </v-simple-table>\n      </v-col>\n    </v-row>\n  </v-container>\n</template>\n\n<script>\nimport Vue from \"vue\";\nimport axios from \"axios\";\n\nVue.use(axios);\n\nexport default {\n  name: \"EmployeeCrud\",\n  data() {\n    return {\n      result: {},\n      employee: {\n        _id: \"\",\n        name: \"\",\n        address: \"\",\n        phone: \"\",\n      },\n    };\n  },\n  created() {\n    this.EmployeeLoad();\n  },\n  mounted() {\n    console.log(\"mounted() called.......\");\n  },\n\n  methods: {\n    EmployeeLoad() {\n      var page = \"http://localhost:8000/user/getAll\";\n      axios.get(page).then(({ data }) => {\n        console.log(data);\n        this.result = data.data;\n      });\n    },\n\n    save() {\n      if (this.employee._id == \"\") {\n        this.saveData();\n      } else {\n        this.updateData();\n      }\n    },\n    saveData() {\n      axios\n        .post(\"http://localhost:8000/user/create\", this.employee)\n        .then(() => {\n          alert(\"saveddddd\");\n          this.EmployeeLoad();\n        });\n    },\n    edit(employee) {\n      this.employee = employee;\n    },\n    updateData() {\n      var editrecords =\n        \"http://localhost:8000/user/update/\" + this.employee._id;\n      axios.patch(editrecords, this.employee).then(() => {\n        this.employee.name = \"\";\n        (this.employee.address = \"\"), (this.employee.phone = \"\");\n        this.id = \"\";\n        alert(\"Updated!!!\");\n        this.EmployeeLoad();\n      });\n    },\n\n    remove(employee) {\n      var url = `http://localhost:8000/user/delete/${employee._id}`;\n      axios.delete(url);\n      alert(\"Deleteddd\");\n      this.EmployeeLoad();\n    },\n  },\n};\n</script>\n"],"mappings":"AAuEA,OAAAA,GAAA;AACA,OAAAC,KAAA;AAEAD,GAAA,CAAAE,GAAA,CAAAD,KAAA;AAEA;EACAE,IAAA;EACAC,KAAA;IACA;MACAC,MAAA;MACAC,QAAA;